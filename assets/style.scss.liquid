// 3.4 GOOGLE MAPS /////////////////////////////////////////////////////////////////////////////////////////////////
#google_map,
#google_map iframe {width: 100% !important;height: 300px !important;background: #e7e7e6;}

#google_map img {max-width: none !important;}




// 3.5 FANCYBOX COMMON STYLES //////////////////////////////////////////////////////////////////////////////////////
.fancybox-skin {padding: 15px;
@include border_radius(0);
}

.fancybox-close {display: block;background: $color_1;border: 3px solid rgba(255,255,255,.9);
	@include border_radius(50%);
	@include box_shadow(0 0 8px 0 rgba(0,0,0,.5));

	&:hover {background: $color_2;}
	&:before {content: '\f00d';width: 30px;height: 30px;display: block;font: 15px/30px 'FontAwesome';color: $color_3;text-align: center;}
}

.fancybox-nav {
	span {width: 36px;height: 36px;display: block;margin: -18px 0 0 0;background: $color_1;border: 3px solid rgba(255,255,255,.9);
	@include border_radius(50%);
	@include box_shadow(0 0 2px 0 rgba(0,0,0,.6));
	@include transition(.3s, background, border-color, color);

		&:before {width: 30px;height: 30px;display: block;font: 20px/28px 'FontAwesome';color: $color_3;text-align: center;}
	}

	&:hover > span {background: $color_2;}


}

.fancybox-prev span:before {content: '\f104';padding: 0 2px 0 0;}
.fancybox-next span:before {content: '\f105';padding: 0 0 0 2px;}




// 3.6 ADD TO CART MESSAGE /////////////////////////////////////////////////////////////////////////////////////////
#cart_added {
	.cart_added__row {height: 120px;display: table;}
	h4 {margin: 0 0 15px 0;text-align: center;}

	&.cart_error {max-width: 420px;}
}

.cart_added__1 {width: 120px;height: 100%;display: table-cell;text-align: center;
	img {height: 120px;padding: 10px;border: 1px solid $border_color;}
}

.cart_added__2 {width: 300px;height: 100%;display: table-cell;padding: 0 0 0 15px;
	.product_name {display: block;}
	#cart_added__quantity {margin: 15px 0 0 0;font-size: 16px;}
	.btn {margin: 15px 10px 0 0;}
}




// 3.7 PRODUCT QUICK VIEW //////////////////////////////////////////////////////////////////////////////////////////
#quick_view__wrap {
	.fancybox-skin {padding: 30px !important;}
}

#product_quick_view {width: 668px;}

.product_quick_wrapper {width: 100%;display: table;}

.quick_view__left {width: 317px;height: 100%;display: table-cell;padding: 0 15px 0 0;
	#img_big {width: 302px;padding: 2px;border: 1px solid $border_color;
		img {background: rgba(0,0,0,.05);}
	}

	.product_images {width: 302px;}
}

.quick_view__right {height: 100%;display: table-cell;overflow: hidden;padding: 0 0 0 15px;
	p {margin: 15px 0 0 0;font-size: 14px;}
	label {font-weight: normal;font-size: 14px;color: $color_4;}
}

#quick_view__name {margin: 0;}

#quick_view__type {}

#quick_view__vendor {}

#quick_view__variants {overflow: hidden;margin: 15px -10px 0 -10px;
	label {display: block;padding: 0 10px;}
	.options_wrapper {}
	.selector-wrapper {width: 33.33333333333333%;float: left;padding: 15px 10px 0 10px;
		label {display: none !important;}
		select {width: 100%;display: block;}
	}
}

#quick_view__price {margin: 30px 0 0 0;position: relative;
	.price {margin: 0;padding: 0 60px 0 0;}
	.money {margin: 0 7px 0 0;font-size: 20px;}
	.money_sale {font-size: 16px;}
	.money_sale_percent {width: 52px;height: 26px;display: block;margin: -13px 0 0 0;background: {{ settings.product_price_color }};font: bold 14px/26px 'Arial';color: $color_3;text-align: center;position: absolute;right: 0;top: 50%;
	@include border_radius(2px);
	}
