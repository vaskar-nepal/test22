#quick_view__availability {margin: 20px 0 0 0;}

#quick_view__form {margin: 15px 0 0 0;
	label {display: block;}
	#quick_view__add {height: 50px;margin: 10px 10px 0 0;padding: 0 20px;font-size: 18px;}
}



////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 4. TYPOGRAPHY ///////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

body {font: normal $base_font_size $base_font;color: $base_font_color;}




// 4.1 HEADINGS ////////////////////////////////////////////////////////////////////////////////////////////////////
h1,
h2,
h3,
h4,
h5,
h6 {margin: 15px 0;font: bold $heading_font_size $heading_font;color: $heading_font_color;text-transform: uppercase;}

h2,
h3,
h4 {font-size: 16px;}

h5,
h6 {font-size: 14px;}

.page_heading {margin: 0;font-size: $heading_font_size;text-align: center;
	a {font-weight: inherit;}
}




// 4.2 PARAGRAPHS & LISTS //////////////////////////////////////////////////////////////////////////////////////////
p {margin: 15px 0;padding: 0;}

.column_center {
	ul {list-style-type: square;list-style-position: inside;}
	ol {list-style-type: decimal;list-style-position: inside;}
}




// 4.3 LINKS ///////////////////////////////////////////////////////////////////////////////////////////////////////
a {color: {{ settings.link_color }};text-decoration: none;
	@include transition(.3s, background, border-color, color);

	&:hover,
	&:active {color: {{ settings.link_color_hover }};}
}




// 4.4 PRODUCT ATTRIBUTES (name, description, price) ///////////////////////////////////////////////////////////////
.product_name {font-weight: bold;font-size: {{ settings.product_name_size }};color: $color_1;
	a {font-weight: bold;color: $color_1;
		@include transition(.3s);
		&:hover {color: $color_2;}
	}
}


.product_desc {font-size: {{ settings.product_desc_size }};color: {{ settings.product_desc_color }};}


.money {color: {{ settings.product_price_color }};}

.money_sale,
.compare-at-price {color: $base_font_color;text-decoration: line-through;}




// 4.5 RTE /////////////////////////////////////////////////////////////////////////////////////////////////////////
.rte {margin-top: 15px;margin-bottom: 15px;color: inherit;
	& > * {max-width: 100% !important;}

	a {text-decoration: underline;
		&:hover {text-decoration: none;}
	}

	h1,
	h2,
	h3,
	h4,
	h5,
	h6 {margin: 30px 0 15px 0;padding: 0 0 15px 0;border-bottom: 2px solid rgba(0,0,0,.2);}

	h1 {font-size: 22px;}
	h2 {font-size: 18px;}
	h3 {font-size: 16px;}
	h4 {font-size: 15px;}
	h5 {font-size: 14px;}
	h6 {font-size: 12px;}

	ul {list-style-type: disc;list-style-position: inside;}
	ol {list-style-type: decimal;list-style-position: inside;}

	blockquote {padding: 5px 35px;font-weight: normal;
		&:before {content: '\f10d';font: 16px 'FontAwesome';}
		&:after {content: '\f10e';float: right;font: 12px 'FontAwesome';}
		& * {margin: 10px 0 !important;font-size: 15px;}
	}

	iframe {max-width: 100% !important;}
}


.page_content {
	.rte {
		table {width: 100%;max-width: 100%;
			td {padding: 0 15px;
				img {width: auto;max-width: 100% !important;}
			}

			h2,
			h3,
			h4,
			h5,
			h6 {margin: 15px 0;padding: 0;border: none;font-weight: normal;color: $color_5;text-transform: uppercase;}

			h4 {font-size: 18px;}
		}
	}
}




////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 5. FORMS & COMPONENTS ///////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// 5.1 FORMS ///////////////////////////////////////////////////////////////////////////////////////////////////////
label {font-weight: bold;font-size: $base_font_size;}

select,
select.form-control,
textarea,
textarea.form-control,
input[type="text"],
input[type="email"],
input[type="number"],
input[type="password"],
input[type="telephone"] {padding: 6px;border:1px solid $border_color;font: normal 13px 'Arial';color: $base_font_color;
-webkit-box-sizing: border-box;
		box-sizing: border-box;

@include border_radius(0);
@include box_shadow(none);
}

select,
select.form-control {padding: 8px 3px;}

textarea,
textarea.form-control {min-width: 100%;max-width: 100%;min-height: 100px;}

select:focus,
select.form-control:focus,
textarea:focus,
textarea.form-control:focus,
input[type="text"]:focus,
input[type="email"]:focus,
input[type="number"]:focus,
input[type="password"]:focus,
input[type="telephone"]:focus {border-color: #aaa;
@include box_shadow(inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #ddd);
}


input[type="radio"],
input[type="checkbox"],
.form-group {vertical-align: top !important;margin-top: 15px;margin-bottom: 0;}


.form-horizontal {
	label {padding-top: 0 !important;text-align: right;line-height: 34px;}
	select {width: 100%;}
}

.action_bottom span.note {margin-left: 5px;}
.action_bottom span.note a {margin-left: 5px;}




// 5.2 ALERTS & NOTIFICATIONS //////////////////////////////////////////////////////////////////////////////////////
.alert {margin: 15px 0;padding: 12px;font-weight: bold;
@include border_radius(0);

	p {margin: 0;}

	a {font-weight: bold;text-decoration: underline;
		&:hover {text-decoration: none;}
	}

	ul {list-style-type: disc;list-style-position: inside;margin: 0;
		li {font-weight: bold;}
	}

	ol {list-style-type: decimal;list-style-position: inside;margin: 0;
		li {font-weight: bold;}
	}

	& > p + p,
	& > ul + ul {margin: 10px 0 0 0;}

}

.alert-success {background-color: #dff0d8;color: #1e892a;
	a {color: #1e892a;}
}

.alert-info {background-color: #cfedfc;color: #267abe;
	a {color: #267abe;}
}

.alert-warning {background-color: #fff5c7;color: #c87100;
	a {color: #c87100;}
}

.alert-danger,
.alert-error {background-color: #ffcccc;color: #d10300;}

.alert-danger a,
.alert-error a {color: #d10300;}


.alert-form {border-color: #d10300 !important;}
.alert-form-info {display: none;margin: 5px 0 0 0;font-weight: bold;color: #d10300;}


.notify_success {color: #1e892a;}
.notify_info {color: #267abe;}
.notify_warning {color: #c87100;}
.notify_danger {color: #d10300;}




// 5.3 BUTTONS /////////////////////////////////////////////////////////////////////////////////////////////////////
.btn,
.btn a,
.btn-info,
.btn-primary {padding: 7px 10px;background: $color_1 !important;border-color: $color_1 !important;font-size: 12px;color: $color_3 !important;
@include border_radius(0);
@include box_shadow(none !important);
@include transition(.3s, background, border-color, color);
}

.btn a {padding: 0 !important;border: none !important;}

.btn:hover,
.btn:hover a,
.btn-info:hover,
.btn-primary:hover {background: $color_2 !important;border-color: $color_2 !important;}

.btn-alt,
.btn-alt a {background: $color_1 !important;border-color: $color_1 !important;}

.btn-alt:hover,
.btn-alt:hover a {background: $color_2 !important;border-color: $color_2 !important;}


.btn-riad {font-size: 18px;background: none !important;border-color: none !important;border: 3px solid $color_3 ! important;font-weight: bold;letter-spacing: 1.5px;padding: 13px 30px;
  position: relative;
  overflow: hidden;
  -webkit-transition: 0.5s;
  transition: 0.5s;

  &:before,
  &:after {
    content: '';
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 0 0 0;
    position: absolute;
    -webkit-transition: 0.5s;
    transition: 0.5s;
    z-index: -1;
  }

  &:after {
    border-color: transparent $color_3 transparent transparent;
    top: 0;
    right: 0;
  }

  &:before {
    border-color: transparent transparent transparent $color_3;
    bottom: 0;
    left: 0;
  }

  &:hover {
    color: $color_1 !important;
    border: 3px solid $color_3 ! important;
    &:before, &:after {
      border-width: 58px 200px;
    }
  }
}

